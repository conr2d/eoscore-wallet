{
  "name": "eosdk-wallet",
  "version": "0.1.0",
  "description": "A simple wallet for eosio blockchain",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest src/tests/*eosdk*"
  },
  "keywords": [
    "eosio",
    "wallets",
    "keys"
  ],
  "author": "Jeeyong Um <conr2d@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/conr2d/eosdk-wallet.git"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "bs58check": "^2.1.2",
    "elliptic": "^6.5.3",
    "eosjs": "^21.0.3",
    "hash.js": "^1.1.7"
  },
  "devDependencies": {
    "@types/aes-js": "^3.1.1",
    "@types/elliptic": "^6.4.12",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.6.0",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./src/tests/setupJest.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(tsx?)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testRegex": "(/src/.*(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node"
  }
}
